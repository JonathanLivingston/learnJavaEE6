<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>FormHandlerServlet</servlet-name>
		<servlet-class>main.webapp.servlet.FormHandlerServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MultipleValueFieldHandlerServlet</servlet-name>
		<servlet-class>main.webapp.servlet.MultipleValueFieldHandlerServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ConfirmationServlet</servlet-name>
		<servlet-class>main.webapp.servlet.ConfirmationServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RedirectionServlet</servlet-name>
		<servlet-class>main.webapp.servlet.ResponseRedirectionServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>FormHandlerServlet</servlet-name>
		<url-pattern>/formhandlerservlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>MultipleValueFieldHandlerServlet</servlet-name>
		<url-pattern>/multiplevaluefieldhandlerservlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ConfirmationServlet</servlet-name>
		<url-pattern>/confirmationservlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>RedirectionServlet</servlet-name>
		<url-pattern>/redirectionservlet</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>dataentry.html</welcome-file>
	</welcome-file-list>
</web-app>